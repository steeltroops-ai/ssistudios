{
  "name": "ssistudios",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev --turbopack",
    "dev:turbo": "bun --bun next dev --turbopack --experimental-turbo",
    "build": "bun --bun next build --experimental-build-mode=compile",
    "build:turbo": "bun --bun next build --turbopack",
    "build:analyze": "ANALYZE=true bun run build",
    "start": "bun --bun next start",
    "start:turbo": "bun --bun next start --turbopack",
    "lint": "bun --bun next lint",
    "lint:fix": "bun --bun next lint --fix",
    "type-check": "bun tsc --noEmit",
    "type-check:watch": "bun tsc --noEmit --watch",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force .next, out, node_modules/.cache -ErrorAction SilentlyContinue\"",
    "clean:all": "bun run clean && powershell -Command \"Remove-Item -Recurse -Force node_modules, bun.lock -ErrorAction SilentlyContinue\"",
    "optimize-fs": "powershell -Command \"if (Test-Path .next) { attrib -R .next\\* /S /D }\"",
    "postinstall": "bun run type-check"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "balanced-match": "^3.0.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concat-map": "^0.0.2",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.2",
    "lucide-react": "^0.539.0",
    "mongodb": "^6.18.0",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "next": "15.4.6",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.1",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
